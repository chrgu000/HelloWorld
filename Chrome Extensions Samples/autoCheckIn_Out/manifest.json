{
	//Requied
	"manifest_version":2,
	"name":"Auto_Checkin_out",
	"version":"1.0.0",
	"version_name":"1.0.0 beta",
	
	//Recommended
	"description":"To Checkin or Checkout automaticly",
	"icons":{
		"16":"icons/icon16.png",
		"48":"icons/icon48.png",
		"128":"icons/icon128.png"
	},
	
	"browser_action": {
		"default_icon": "icons/icon19.png",
		"default_popup": "pop.html",
		"default_title": "Click here!"
	},
	
	//Optional
	"author":"Tyler_MA",
	"background":{
		//long-running script to manage some task or state
		"scripts":["background.js"],
		"persistent":true
	},
	
	"options_ui":{
		"page":"pop.html",
		"chrome_style":true
	},
	
	"permissions":[
		"tabs",
		"activeTab",
		"https://*.baidu.com/*",
		"http://*.narutom.com/*"
	],
	//always be injected
	"content_scripts":[
		{
			"matches": ["http://www.narutom.com/*"],
			"js": ["adBlock.js"]
		}
		
	]
	
	
	
	
}